# RPSL common attributes
ATTR	ch	changed
ATTR	de	descr
ATTR	ac	admin-c
ATTR	tc	tech-c
ATTR	mb	mnt-by
ATTR	ny	notify
ATTR	or	origin
ATTR	rm	remarks
ATTR	so	source
ATTR	ph	phone
ATTR	fx	fax-no
ATTR	em	e-mail
ATTR	nh	nic-hdl

# RFC2769 attributes -- not used yet
ATTR	dl	delegated
ATTR	it	integrity

# RFC2725 attributes -- not used yet
ATTR	ml	mnt-lower
ATTR	mr	mnt-routes
ATTR	rl	reclaim
ATTR	nr	no-reclaim
ATTR	rb	referral-by
ATTR	ay	auth-override

# repository fields
ATTR	re	repository
ATTR	qa	query-address
ATTR	ru	response-auth-type
ATTR	sa	submit-address
ATTR    su      submit-auth-type
ATTR	rc	repository-cert
ATTR	ep	expire
ATTR	hi	heartbeat-interval

# domain fields
ATTR	dn	domain
ATTR	zc	zone-c
ATTR	ns	nserver
ATTR	sd	sub-dom
ATTR	di	dom-net
ATTR	rf	refer

# inetnum fields
ATTR	in	inetnum
ATTR	na	netname
ATTR	cy	country
ATTR	rz	rev-srv
ATTR	st	status

# inet6num fields
ATTR	i6	inet6num

# key-cert fields
ATTR	kc	key-cert
ATTR	mh	method
ATTR	ow	owner
ATTR	fp	fingerpr
ATTR	ce	certif

# RPSL dictionary fields
ATTR    dc      dictionary
ATTR    td      typedef
ATTR	rp	rp-attribute
ATTR    pl      protocol
# RPSLng introduces afi dictionary attribute
ATTR	af	afi

# route / route6
ATTR    rt      route
ATTR    r6      route6
ATTR	ho	holes
ATTR	ij	inject         
ATTR	mo	member-of 
ATTR 	co	components  
ATTR	ab	aggr-bndry
ATTR	ag	aggr-mtd
ATTR	ec	export-comps

# aut-num fields
# RPSLng introduces mp-import and mp-export
ATTR	an	aut-num
ATTR    aa  	as-name
ATTR    ip      import
ATTR	mi	mp-import
ATTR    ex      export
ATTR	mx	mp-export
ATTR	df	default
ATTR	md	mp-default
ATTR	ma	member-of

# as-block fields
ATTR	ak	as-block

# inet-rtr fields
ATTR    ir      inet-rtr
ATTR    az      alias
ATTR    la      local-as
ATTR    if      ifaddr
ATTR    pe      peer
ATTR	mz	member-of
# mp-peer and interface in RPSLng attribute
ATTR    pm      mp-peer
ATTR	ie	interface
# Route Server specific attributes for inet-rtr class -ljb
ATTR	ri	rs-in
ATTR	rx	rs-out

# set common attrs
ATTR    my      mbrs-by-ref

# as-set fields
ATTR    as      as-set 
ATTR	ms	members

# route-set fields
ATTR    rs      route-set
ATTR	me	members
# mp-members is a RPSLng attribute
ATTR    mm	mp-members

# rtr-set fields
ATTR    is      rtr-set
ATTR	mg	members
# mp-members is a RPSLng attribute
ATTR    mj	mp-members

# peering-set fields
ATTR	ps	peering-set
ATTR	pg	peering
# mp-peering for RPSLng
ATTR	mp	mp-peering

# filter-set fields
ATTR	fs	filter-set
ATTR	fi	filter
# mp-filter for RPSLng
ATTR	mf	mp-filter

# person fields
ATTR	pn	person
ATTR	ad	address

# role fields
ATTR	ro	role
ATTR	tb	trouble

# mntner fields
ATTR	mt	mntner
ATTR	dt	upd-to
ATTR	mn	mnt-nfy
ATTR	at	auth

# ipv6-site - 6bone object - no parser support, largely redundant with RPSLng
ATTR	s6	ipv6-site
ATTR	lo	location
ATTR	pr	prefix
ATTR	ap	application
ATTR	tu	tunnel
ATTR	ct	contact
ATTR	ul	url

# limerick fields - RIPE humor - unlikely to be supported by parser
ATTR	li	limerick
ATTR	te	text
ATTR	au	author

#
# Attributes with u* short names are special!
# They all have hardcoded side effects, so do NOT change them
# unless you know what you are doing

ATTR	ud	delete		# delete operation
ATTR	uo	override	# override all security for updates
ATTR	up	password	# password
ATTR	uc	cookie		# system info

#	aut-num
#
#OBJ	an	ATSQ	an aa de ma ip mi ex mx df md ml mr rl nr dl ac tc
OBJ	an	ATSQ	an aa de ma ip mi ex mx df md ac tc
OBJ	an	ATSQ	rm ny mb ch so
OBJ	an	ATSQ	ud uo up uc
OBJ     an      MAND    an aa de ac tc mb ch so
OBJ     an      MULT    ma ip mi ex mx df md ac tc rm ny mb ch
#OBJ     an      MULT    ma ip mi ex mx df md ac tc rm ny ml mr rl nr dl mb ch

#	as-set
#
OBJ     as      ATSQ    as de ms my ac tc
OBJ	as	ATSQ	rm ny mb ch so
OBJ	as	ATSQ	ud uo up uc
OBJ     as      MAND    as de mb ch so
OBJ     as      MULT    ms my ac tc rm ny ch mb

#       mntner
#
#OBJ     mt      ATSQ    mt de ac tc dt mn at ay rb
OBJ     mt      ATSQ    mt de ac tc dt mn at
OBJ	mt	ATSQ	rm ny mb ch so
OBJ	mt	ATSQ	ud uo up uc
OBJ     mt      MAND    mt de ac dt at mb ch so
OBJ     mt      MULT    ac tc dt mn at rm ny mb ch

